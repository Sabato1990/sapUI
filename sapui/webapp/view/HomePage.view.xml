<mvc:View controllerName="com.webinar.sapui.controller.HomePage"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
<Page id="Products" title="Available Products">
    <content>
      <List items="{/Products}" id="Items">
        <StandardListItem type="Active" title="{ProductName}" id="ListItem" />
      </List>
    </content>
  </Page>
</mvc:View>
